<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/image1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/report_bg_calendar_year"
        android:layout_alignParentRight="true"
        android:visibility="gone"/>


    <ImageView
        android:id="@+id/image2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/report_bg_calendar_mouth"
        android:layout_alignParentRight="true"
        android:visibility="gone"/>

    <ImageView
        android:id="@+id/image3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/report_bg_calendar_day"
        android:layout_alignParentRight="true"
        android:visibility="gone"/>

    <ImageView
        android:id="@+id/image4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/report_bg_calendar_ok"
        android:layout_alignParentRight="true"
        android:visibility="gone"/>

    <com.szip.sleepee.View.CircleMenuLayout
        android:id="@+id/id_menulayout1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="100dp"
        android:visibility="gone"
        >

    </com.szip.sleepee.View.CircleMenuLayout>

</RelativeLayout>